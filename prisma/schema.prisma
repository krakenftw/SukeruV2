generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  userId String
  joinMessage String?
  joinMessageTwo String?
  lastMessage String?
  lastUpdated String?
  joined DateTime?
  xp Int @default(0)
  level Int @default(1)
}
